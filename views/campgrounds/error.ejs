<% layout('layouts/boilerplate') %>

<div class="row">
  <div class="col-md-8 offset-md-2 mb-5">
    <img
      src="https://images.unsplash.com/photo-1589652717521-10c0d092dea9?ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D&auto=format&fit=crop&w=1170&q=80"
      alt="error - dog photo"
      class="img-fluid mt-auto rounded mb-5"
    />
    <div class="alert alert-danger text-center mb-auto" role="alert">
      <h4 class="alert-heading"><%= err.message %></h4>
      <hr />
      <p class="mb-0">
        <b>Navigate to the home page or contact our support</b>
      </p>
    </div>
  </div>
</div>
