<!-- <% if (success.length || error.length) {%> -->
<div
  class="alert <%= success.length?'alert-success':'alert-danger' %> alert-dismissible fade show text-center fs-5"
  role="alert"
>
  <%= success.length?success:error %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
